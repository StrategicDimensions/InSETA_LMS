<!--                        </group>-->
                        <group string="Work Address" attrs="{'invisible':[('page','!=','address')]}">
                            <field name='street' string="Address Line 1"/>
                            <field name='street2' string="Address Line 2"/>
                            <field name='street3' string="Address Line 3"/>
